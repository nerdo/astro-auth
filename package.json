{
  "name": "astro-auth",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev:ngrok": "concurrently \"npm:surrealdb\" \"npm:dev\" \"npm:ngrok\"",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
